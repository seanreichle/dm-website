<h1>Listing casestudies</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Tagline</th>
    <th>Description</th>
    <th>Goals</th>
    <th>Achievements</th>
    <th>Results</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @casestudies.each do |casestudy| %>
  <tr>
    <td><%= casestudy.title %></td>
    <td><%= casestudy.tagline %></td>
    <td><%= casestudy.description %></td>
    <td><%= casestudy.goals %></td>
    <td><%= casestudy.achievements %></td>
    <td><%= casestudy.results %></td>
    <td><%= link_to 'Show', casestudy %></td>
    <td><%= link_to 'Edit', edit_casestudy_path(casestudy) %></td>
    <td><%= link_to 'Destroy', casestudy, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

